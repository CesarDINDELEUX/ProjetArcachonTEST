        <Label x:Name="LabelPassword" Content="Mot de Passe :" Margin="225,187,238.4,192.4"/>
        <PasswordBox x:Name="PassBox" BorderBrush="Black"  Margin="306,183,238.6,192.6" Height="Auto" Width="Auto" GotFocus="PassBox_GotFocus"/>
