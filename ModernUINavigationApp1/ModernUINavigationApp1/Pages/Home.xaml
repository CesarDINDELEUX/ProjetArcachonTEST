             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" Height="449.5" Width="781.6">
    
    <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer Width="Auto" Height="Auto">
            <StackPanel>
                <Label x:Name="LabelAccueil" Content="Bienvenue sur Projet Arcachon" HorizontalContentAlignment="Center" FontSize="20"  Width="Auto" Height="Auto"/>
